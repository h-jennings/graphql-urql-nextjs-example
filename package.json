{
  "name": "next-starter",
  "version": "1.0.0",
  "scripts": {
    "gen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=.env",
    "dev": "yarn gen && next dev",
    "dev:inspect": "yarn gen && NODE_OPTIONS='--inspect' next dev",
    "build:profile": "next build --profile",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "export": "yarn build && next export",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write 'src/**/*.js'",
    "test": "jest",
    "test:clear-cache": "jest --clearCache"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.0",
    "@next/bundle-analyzer": "^12.1.4",
    "@stitches/react": "^1.2.7",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "next": "^12.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^5.3.0",
    "next-sitemap": "^2.5.17",
    "next-urql": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "urql": "^2.2.0",
    "use-debugger-hooks": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.6.3"
  },
  "eslintIgnore": [
    "public/**",
    "out"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  }
}
