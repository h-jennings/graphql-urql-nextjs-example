{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "@next/next/no-img-element": 1,
    "jsx-a11y/alt-text": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-console": 2,
    "react/no-unused-prop-types": [
      2,
      {
        "skipShapeProps": true
      }
    ],
    "react/self-closing-comp": 2
  }
}
